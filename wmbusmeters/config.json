{
    "name": "Wmbusmeters (W-MBus to MQTT)",
    "version": "release-1.14.0",
    "slug": "wmbusmeters",
    "description": "Acquire utility meter readings using the wireless mbus protocol (WMBUS)",
    "arch": [
        "armv7",
        "aarch64",
        "amd64"
    ],
    "panel_icon": "mdi:home-automation",
    "boot": "auto",
    "init": false,
    "ingress": true,
    "devices": [
        "/dev/ttyUSB0",
        "/dev/ttyAMA0"
    ],
    "usb": true,
    "uart": true,
    "startup": "application",
    "services": [
        "mqtt:need"
    ],
    "map": [
        "share:rw",
        "config:rw"
    ],
    "ports": {
        "9011/udp": null,
        "9022/tcp": null
    },
    "ports_description": {
        "9011/udp": "Netcat udp-listen port",
        "9022/tcp": "Netcat tcp-listen port"
    },
    "options": {
        "reset_config": "no",
        "data_path": "/config/addon/wmbusmeter",
        "mqtt": {},
        "config": {
            "loglevel": "normal",
            "device": "auto:t1",
            "donotprobe": "/dev/ttyAMA0",
            "logtelegrams": "false",
            "format": "json",
            "logfile": "/config/addon/wmbusmeter/logs/",
            "shell": "/wmbusmeters/mosquitto_pub.sh \"wmbusmeters/$METER_NAME\" \"$METER_JSON\""
        }
    },
    "schema": {
        "reset_config": "list(no|yes)",
        "data_path": "str",
        "mqtt": {
            "server": "str?",
            "ca": "str?",
            "key": "str?",
            "cert": "str?",
            "user": "str?",
            "password": "str?"
        },
        "config": {
            "loglevel": "str?",
            "device": "str?",
            "donotprobe": "str?",
            "logtelegrams": "str?",
            "format": "str?",
            "logfile": "str?",
            "shell": "str?"
        }
    }
}